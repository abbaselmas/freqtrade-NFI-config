{
    "pairlists": [
        {
            "method": "VolumePairList",
            "number_assets": 360,
            "sort_key": "quoteVolume",
            "min_value": 5000000,
            "refresh_period": 1800
        },
        {
            "method": "AgeFilter",
            "min_days_listed": 14
        },
        {
            "method": "PriceFilter",
            "min_price": 0.0001,
            "low_price_ratio": 0.01,
            "max_price": 1000,
            "max_value": 1
        },
        {
            "method": "RangeStabilityFilter",
            "lookback_days": 3,
            "min_rate_of_change": 0.03,
            "refresh_period": 1800
        },
        {
            "method": "VolatilityFilter",
            "lookback_days": 3,
            "min_volatility": 0.02,
            "max_volatility": 0.75,
            "refresh_period": 21600
        },
        {
            "method": "OffsetFilter",
            "offset": 2
        }
    ],
    "exchange": {
        "name": "binance",
        "pair_whitelist": [
            ".*/USDT"
        ],
        "pair_blacklist": [
            // Exchange
            "(BNB)/.*",
            // Leverage
            ".*(_PREMIUM|BEAR|BULL|HALF|HEDGE|UP|DOWN|[1235][SL])/.*",
            // Fiat
            "(AUD|BRZ|CAD|CHF|EUR|GBP|HKD|IDRT|JPY|NGN|RUB|SGD|TRY|UAH|USD|ZAR)/.*",
            // Stable
            "(BUSD|CUSD|CUSDT|DAI|PAXG|SUSD|TUSD|USDC|USDN|USDP|USDT|VAI|UST|USTC)/.*",
            // FAN
            "(ACM|AFA|ALA|ALL|APL|ASR|ATM|BAR|CAI|CITY|FOR|GAL|GOZ|IBFK|JUV|LAZIO|LEG|LOCK-1|NAVI|NMR|NOV|OG|PFL|PORTO|PSG|ROUSH|SANTOS|STV|TH|TRA|UCH|UFC|YBO)/.*",
            // Others
            "(1EARTH|ILA|BOBA|CTXC|CWAR|HBAR|NMR|OMG|ONG|ARDR|DMTR|MLS)/.*",
            //ABBAS
            "(CTXC|PNT|TVK|AGLD|APE|VOXEL|AION|REI|KEY|VIDT|IRIS|QUICK|KMD|DREP|POLS|ATM|FIO|ALCX|ANC|BETA|NKN|AVA|WRX|CKB|BEAM|FLM|PERL|DEGO|BTCST)/.*"
        ]
    }
}
