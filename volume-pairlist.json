{
    "pairlists": [
        {
            "method": "VolumePairList",
            "number_assets": 360,
            "sort_key": "quoteVolume",
            "min_value": 10000000,
            "refresh_period": 21600
        },
        {
            "method": "AgeFilter",
            "min_days_listed": 15
        },
        {
            "method": "PriceFilter",
            "min_price": 0.0001,
            "low_price_ratio": 0.01,
            "max_price": 1000,
            "max_value": 1
        },
        {
            "method": "RangeStabilityFilter",
            "lookback_days": 3,
            "min_rate_of_change": 0.03,
            "refresh_period": 21600
        },
        {
            "method": "VolatilityFilter",
            "lookback_days": 3,
            "min_volatility": 0.02,
            "max_volatility": 0.75,
            "refresh_period": 21600
        },
        {
            "method": "PrecisionFilter"
        }
    ],
    "exchange": {
        "name": "binance",
        "pair_whitelist": [
            ".*/USDT"
        ],
        "pair_blacklist": [
            // Exchange
            "(BNB)/.*",
            // Leverage
            ".*(_PREMIUM|BEAR|BULL|HALF|HEDGE|UP|DOWN|[1235][SL])/.*",
            // Fiat
            "(AUD|BRZ|CAD|CHF|EUR|GBP|HKD|IDRT|JPY|NGN|RUB|SGD|TRY|UAH|USD|ZAR)/.*",
            // Stable
            "(BUSD|CUSD|CUSDT|DAI|PAXG|SUSD|TUSD|USDC|USDN|USDP|USDT|VAI|UST|USTC)/.*",
            // FAN
            "(ACM|AFA|ALA|ALL|APL|ASR|ATM|BAR|CAI|CITY|FOR|GAL|GOZ|IBFK|JUV|LAZIO|LEG|LOCK-1|NAVI|NMR|NOV|OG|PFL|PORTO|PSG|ROUSH|SANTOS|STV|TH|TRA|UCH|UFC|YBO)/.*",
            //ABBAS
            "(AION|BTG|ALCX|MDT|IDEX|DNT|MULTI|HIVE|FARM|WING|DATA|TCT|ERN|TORN|BIFI|OM|RIF|MITH|PHA|DEGO|AMP)/.*",
            "(MFT|ZIL|HIGH|NBS|DF|OXT|REEF|BOND|ANC|CTXC|PNT|LAZIO|SYS|ACA|JST|POND|VOXEL|ACH|KEY|QI)/.*",
            "(SRM|MASK|FTT|LUNC)/.*"
        ]
    }
}
