{
    "pairlists": [
        {
            "method": "VolumePairList",
            "number_assets": 360,
            "sort_key": "quoteVolume",
            "min_value": 10000000,
            "refresh_period": 2700
        },
        {
            "method": "AgeFilter",
            "min_days_listed": 15
        },
        {
            "method": "PriceFilter",
            "min_price": 0.0001,
            "low_price_ratio": 0.01,
            "max_price": 200,
            "max_value": 1
        },
        {
            "method": "SpreadFilter",
            "max_spread_ratio": 0.005
        },
        {
            "method": "RangeStabilityFilter",
            "lookback_days": 3,
            "min_rate_of_change": 0.03,
            "refresh_period": 21600
        },
        {
            "method": "VolatilityFilter",
            "lookback_days": 3,
            "min_volatility": 0.015,
            "max_volatility": 0.75,
            "refresh_period": 21600
        },
        {
            "method": "ShuffleFilter",
            "shuffle_frequency": "candle",
            "seed": 73
        }
    ],
    "exchange": {
        "name": "binance",
        "pair_whitelist": [
            ".*/USDT"
        ],
        "pair_blacklist": [
            "(BNB)/.*",
            "(ALCX|LPT|IOTX|ARKM|BSW|STX|QNT|PERP|VANRY|LDO|AR|WOO|KDA|C98|ELF|HOOK|MAV|SKL|OSMO|RARE|KSM|SEI)/.*"
